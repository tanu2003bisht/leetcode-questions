import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		int reverseno=0;
		int n=sc.nextInt();
		while(n>0){
			 int ld = n % 10;
			n = n / 10;
			reverseno = ((reverseno*10)+ ld);
		}
        System.out.print( reverseno);
	}
}
